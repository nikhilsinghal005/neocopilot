{
  "publisher": "vidyutdatalabs",
  "name": "neocopilot",
  "displayName": "Neo Copilot",
  "description": "CodeAssist: Unleash the power of intelligent code suggestions with CodeAssist. Boost your productivity and streamline your coding workflow by harnessing the advanced suggestion engine that understands your code like a pro.",
  "version": "0.0.9",
  "engines": {
    "vscode": "^1.80.0"
  },
  "homepage": "https://www.neocopilot.io",
  "icon": "media/icon.png",
  "categories": [
    "Other", "Programming Languages", "Machine Learning", "AI",
    "Snippets", "Chat", "Education", "Data Science"],
  "keywords": [
    "code-assist", "code-completion", "python", "javascript", "typescript", "java",
    "c#", "c++", "c", "php", "ruby", "go", "swift", "kotlin", "scala", "dart",
    "rust", "r", "sql", "html", "css", "json", "yaml", "xml", "google", "chatgpt",
    "gpt4", "chat"],
  "activationEvents": [
    "onUri",
    "onLanguage:python",
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "disable-code-suggestions",
        "title": "Disable Code Suggestions"
      }
    ]
  },
  "scripts": {
    "build": "node esbuild.js",
    "watch": "node esbuild.js --watch",
    "lint": "eslint 'src/**/*.{ts,tsx}'"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/diff": "^5.2.1",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/uuid": "^9.0.8",
    "@types/vscode": "^1.80.0",
    "@types/vscode-webview": "^1.57.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "esbuild": "^0.16.10",
    "esbuild-plugin-copy": "^2.0.1",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.1.3",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@vscode/codicons": "^0.0.35",
    "@vscode/webview-ui-toolkit": "^1.4.0",
    "axios": "^1.7.2",
    "diff": "^5.2.0",
    "highlight.js": "^11.9.0",
    "socket.io-client": "^4.7.5",
    "uuid": "^9.0.1",
    "weather-js": "^2.0.0"
  }
}
