{
  "publisher": "vidyutdatalabs",
  "name": "neocopilot",
  "displayName": "Neo Copilot",
  "description": "CodeAssist provides developers with intelligent, context-sensitive code suggestions designed to boost productivity and simplify the development process. Harness the power of an advanced suggestion engine that understands your code, delivering seamless and efficient workflow support.",
  "version": "0.100.22",
  "engines": {
    "vscode": "^1.70.0"
  },
  "homepage": "https://www.neocopilot.io",
  "icon": "assets/icon.png",
  "categories": [
    "Programming Languages",
    "Machine Learning",
    "AI",
    "Snippets",
    "Chat",
    "Education",
    "Data Science"
  ],
  "keywords": [
    "AI Code Assistant",
    "Code Completion",
    "Intelligent Code Suggestions",
    "Programming Helper",
    "AI-Powered Coding",
    "javascript",
    "typescript",
    "ChatGPT",
    "GEMINI",
    "Chat",
    "AI-Powered Coding",
    "Developer Productivity",
    "Machine Learning Code Tools",
    "Automated Code Writing",
    "AI Debugging Tools"
  ],
  "activationEvents": [
    "onUri",
    "onStartupFinished",
    "onLanguage:python"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "uriHandlers": [
      {
        "uri": "vscode://neocopilot/token"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "codebuddy",
          "title": "NeoChat",
          "icon": "assets/icon.svg"
        }
      ]
    },
    "views": {
      "codebuddy": [
        {
          "type": "webview",
          "id": "aiChatPanelPrimary",
          "name": "Neo Chat Panel",
          "icon": "assets/icon.svg",
          "retention": true
        }
      ]
    },
    "commands": [
      {
        "command": "disable-code-suggestions",
        "title": "Disable Code Suggestions"
      },
      {
        "command": "extension.neoEdit",
        "title": "Neo Edit"
      },
      {
        "command": "extension.neoChatInsert",
        "title": "Neo Chat Insert"
      },
      {
        "command": "neocopilot.showSettings",
        "title": "Settings",
        "icon": "$(gear)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "neocopilot.showSettings",
          "when": "view == aiChatPanelPrimary",
          "group": "navigation"
        }
      ]
    },
    "keybindings": [
      {
        "command": "extension.neoEdit",
        "key": "ctrl+I",
        "when": "editorTextFocus"
      },
      {
        "command": "extension.neoChatInsert",
        "key": "ctrl+O",
        "when": "editorTextFocus"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Neo Copilot Configuration",
      "properties": {
        "neocopilot.languageFunctionsEnabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable or disable the Language Functions CodeLenses."
        },
        "neocopilot.languageFunctionsDebounceDelay": {
          "type": "number",
          "default": 200,
          "description": "Debounce delay in milliseconds for Language Functions CodeLens refresh."
        }
      }
    }
  },
  "scripts": {
    "compile": "tsc -p ./",
    "build": "node esbuild.js",
    "watch": "node esbuild.js --watch",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "prepare": "husky install",
    "test": "echo \"No tests specified\""
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/axios": "^0.14.0",
    "@types/diff": "^5.2.1",
    "@types/glob": "^8.1.0",
    "@types/lodash": "^4.17.7",
    "@types/mocha": "^10.0.1",
    "@types/node": "^22.9.1",
    "@types/uuid": "^9.0.8",
    "@types/vscode": "^1.70.0",
    "@types/vscode-webview": "^1.57.5",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.36.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vscode/test-electron": "^2.3.2",
    "clangd": "^0.0.0",
    "esbuild": "^0.16.17",
    "esbuild-plugin-copy": "^2.0.1",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "gopls": "^0.0.1-security.5",
    "husky": "^9.1.7",
    "intelephense": "^1.12.6",
    "lint-staged": "^16.1.4",
    "mocha": "^10.2.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.3",
    "vite": "^5.4.8",
    "vscode-languageserver-textdocument": "^1.0.12",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "diff": "^5.2.0",
    "dotenv": "^16.4.5",
    "fast-diff": "^1.3.0",
    "lodash": "^4.17.21",
    "socket.io-client": "^4.7.5",
    "uuid": "^9.0.1",
    "vscode-languageclient": "^8.1.0",
    "vscode-languageserver": "^8.1.0",
    "weather-js": "^2.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --fix"
  }
}
